<resources>
    
    <!-- Generated with the following settings
    http://jgilfelt.github.io/android-actionbarstylegenerator
    name=ThemeActionBar
    compat=appcompat
    theme=dark
    actionbarstyle=transparent
    texture=0
    hairline=0
    neutralPressed=0
    backColor=2D2D2D%2C100
    secondaryColor=151515%2C100
    tabColor=312940%2C100
    tertiaryColor=303030%2C100
    accentColor=312940%2C100
    cabBackColor=000%2C100
    cabHighlightColor=312940%2C100
     -->

    <style name="Theme.Themeactionbar" parent="@style/Theme.AppCompat">
        <item name="actionBarItemBackground">@drawable/selectable_background_themeactionbar</item>
        <item name="popupMenuStyle">@style/PopupMenu.Themeactionbar</item>
        <item name="dropDownListViewStyle">@style/DropDownListView.Themeactionbar</item>
        <item name="actionBarTabStyle">@style/ActionBarTabStyle.Themeactionbar</item>
        <item name="actionDropDownStyle">@style/DropDownNav.Themeactionbar</item>
        <item name="actionBarStyle">@style/ActionBar.Transparent.Themeactionbar</item>
        <item name="actionModeBackground">@drawable/cab_background_top_themeactionbar</item>
        <item name="actionModeSplitBackground">@drawable/cab_background_bottom_themeactionbar</item>
        <item name="actionModeCloseButtonStyle">@style/ActionButton.CloseMode.Themeactionbar</item>

        
    </style>

    <style name="ActionBar.Solid.Themeactionbar" parent="@style/Widget.AppCompat.ActionBar.Solid">
        <item name="background">@drawable/ab_solid_themeactionbar</item>
        <item name="backgroundStacked">@drawable/ab_stacked_solid_themeactionbar</item>
        <item name="backgroundSplit">@drawable/ab_bottom_solid_themeactionbar</item>
        <item name="progressBarStyle">@style/ProgressBar.Themeactionbar</item>
    </style>

    <style name="ActionBar.Transparent.Themeactionbar" parent="@style/Widget.AppCompat.ActionBar">
        <item name="background">@drawable/ab_transparent_themeactionbar</item>
        <item name="progressBarStyle">@style/ProgressBar.Themeactionbar</item>
    </style>
	
    <style name="PopupMenu.Themeactionbar" parent="@style/Widget.AppCompat.PopupMenu">	
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_themeactionbar</item>	
    </style>
	
    <style name="DropDownListView.Themeactionbar" parent="@style/Widget.AppCompat.ListView.DropDown">
        <item name="android:listSelector">@drawable/selectable_background_themeactionbar</item>
    </style>

    <style name="ActionBarTabStyle.Themeactionbar" parent="@style/Widget.AppCompat.ActionBar.TabView">
        <item name="android:background">@drawable/tab_indicator_ab_themeactionbar</item>
    </style>
	
    <style name="DropDownNav.Themeactionbar" parent="@style/Widget.AppCompat.Spinner.DropDown.ActionBar">
        <item name="android:background">@drawable/spinner_background_ab_themeactionbar</item>
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_themeactionbar</item>
        <item name="android:dropDownSelector">@drawable/selectable_background_themeactionbar</item>
    </style>
    
    <style name="ProgressBar.Themeactionbar" parent="@style/Widget.AppCompat.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/progress_horizontal_themeactionbar</item>
    </style>
    
    <style name="ActionButton.CloseMode.Themeactionbar" parent="@style/Widget.AppCompat.ActionButton.CloseMode">
        <item name="android:background">@drawable/btn_cab_done_themeactionbar</item>
    </style>
    
    <!-- this style is only referenced in a Light.DarkActionBar based theme -->
    <style name="Theme.Themeactionbar.Widget" parent="@style/Theme.AppCompat">
        <item name="popupMenuStyle">@style/PopupMenu.Themeactionbar</item>
        <item name="dropDownListViewStyle">@style/DropDownListView.Themeactionbar</item>
    </style>

</resources>